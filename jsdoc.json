{
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc", "closure"]
	},
	"source": {
		"include": ["./src", "./index.html"],
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"sourceType": "module",
	"plugins": [
		"plugins/markdown",
		"better-docs/component",
		"better-docs/category"
	],
	"opts": {
		"encoding": "utf8",
		"destination": "docs/",
		"recurse": true,
		"template": "node_modules/better-docs",
		"readme": "README.md"
	},
	"templates": {
        "cleverLinks": true,
        "monospaceLinks": false,
		"search": true,
		"better-docs": {
			"name": "SportSee"
		}
    }
}